<Window x:Class="EgetForsoeg1Scenarie.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EgetForsoeg1Scenarie"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid VerticalAlignment="Top">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        
        
        <StackPanel Orientation="Horizontal" Grid.Row="0" Height="35">
            <Button x:Name="btn_Forside" Content="Forside" MinWidth="100" Click="BtnClickForside"/>
            <Button x:Name="btn_Ny_Film" Content="Ny film" MinWidth="100" Margin="10,0,0,0" Click="BtnClickNyFilm"/>
        </StackPanel>

        <Frame x:Name="Main" Grid.Row="1" Margin="0,10,0,0"/>
        
    </Grid>
</Window>
